{
  "name": "total-hemp-consumables",
  "version": "0.0.1",
  "description": "Backend for Total Hemp Consumables, A Lifestyle Family Farms brand.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "medusa build",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "seed:tsv": "ts-node ./src/scripts/seed-from-tsv.ts",
    "seed:tsv:apply": "SEED_DRY_RUN=0 ts-node ./src/scripts/seed-from-tsv.ts",
    "seed:prod": "ts-node ./src/scripts/seed-remote.ts",
    "seed:dev": "NODE_ENV=development ts-node ./src/scripts/seed-remote.ts",
    "shipstation:doctor:test": "SHIPSTATION_ENV=test DISABLE_MEDUSA_ADMIN=true yarn medusa exec ./src/scripts/shipstation-doctor.ts",
    "shipstation:doctor:prod": "SHIPSTATION_ENV=production DISABLE_MEDUSA_ADMIN=true yarn medusa exec ./src/scripts/shipstation-doctor.ts",
    "shipstation:inspect:test": "SHIPSTATION_ENV=test DISABLE_MEDUSA_ADMIN=true yarn medusa exec ./src/scripts/inspect-shipstation.ts",
    "shipstation:inspect:prod": "SHIPSTATION_ENV=production DISABLE_MEDUSA_ADMIN=true yarn medusa exec ./src/scripts/inspect-shipstation.ts",
    "shipstation:sync:test": "SHIPSTATION_ENV=test DISABLE_MEDUSA_ADMIN=true yarn medusa exec ./src/scripts/create-missing-shipstation-options.ts",
    "shipstation:sync:prod": "SHIPSTATION_ENV=production DISABLE_MEDUSA_ADMIN=true yarn medusa exec ./src/scripts/create-missing-shipstation-options.ts",
    "mirror:dev": "ts-node ./src/scripts/mirror-prod-to-dev.ts",
    "mirror:inventory": "ts-node ./src/scripts/mirror-inventory.ts",
    "db:reset:dev": "ts-node ./src/scripts/db-reset-dev.ts && medusa db:migrate",
    "rebuild:dev": "yarn db:reset:dev && echo 'Run the manual admin checklist (docs/admin-store-setup.md) before seeding catalog data.'",
    "rebuild:dev:config": "yarn db:reset:dev && ALLOW_CONFIG_SEED=true medusa exec ./src/scripts/seed-config.ts",
    "mirror:dev:all": "yarn mirror:dev && yarn mirror:inventory",
    "start": "medusa start",
    "dev": "medusa develop",
    "predeploy": "medusa db:migrate",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit"
  },
  "dependencies": {
    "@medusajs/admin-sdk": "^2.13.0",
    "@medusajs/cli": "^2.13.0",
    "@medusajs/framework": "^2.13.0",
    "@medusajs/js-sdk": "^2.13.0",
    "@medusajs/medusa": "^2.13.0",
    "@medusajs/types": "^2.13.0",
    "@opentelemetry/exporter-trace-otlp-http": "0.200.0",
    "awilix": "^8.0.1",
    "axios": "^1.9.0",
    "payment-authorizenet-medusa": "^1.0.1",
    "posthog-node": "^5.11.0",
    "yoctocolors-cjs": "^2.1.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@medusajs/test-utils": "^2.13.0",
    "@swc/core": "^1.7.28",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vite": "^5.4.21",
    "yalc": "^1.0.0-pre.53"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "yarn@4.9.1"
}
